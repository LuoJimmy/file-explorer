    "axios": "^1.9.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "nodemon": "^3.0.1",
    "pinia": "^2.1.6",
    "sass": "^1.67.0",
    "vite": "^4.4.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
